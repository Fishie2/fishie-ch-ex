{
  "name": "Fishie",
  "short_name": "Fishe",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Tools to fight the disinformation overload on social media.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "Fishie",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ]
 
}
